{
  "name": "cottage",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:build": "next build",
    "start": "next start",
    "export": "next export",
    "build": "npm run dev:build && npm run export",
    "deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "watch": "tailwindcss -i ./src/data/main.css -o ./dist/main.css --watch"
  },
  "dependencies": {
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.4.2",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.16",
    "typescript": "^4.5.5"
  }
}
